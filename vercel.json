{
  "version": 2,
  "builds": [
    {
      "src": "/desloSite",
      "use": "@vercel/static"
    },
    {
      "src": "desloSite/ryonv-back/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/desloSite"
    },
    {
      "src": "/api/(.*)",
      "dest": "desloSite/ryonv-back/server.js"
    }
  ]
}
